{
  "manifest_version": 3,
  "name": "ETH Miner Extension",
  "version": "1.0",
  "description": "Mine ETH while browsing with this extension.",
  "permissions": [
    "tabs",
    "background"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_key": "value"
}
